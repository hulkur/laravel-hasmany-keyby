{
    "name": "hulkur/laravel-hasmany-keyby",
    "description": "Add possibility to have related models in many-to-many relationship attached to the parent model by defined key values.",
    "homepage": "https://github.com/hulkur/laravel-hasmany-keyby",
    "license": "MIT",
    "require": {
        "php" : "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.0",
        "doctrine/persistence": "^2.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^9.0"
    },
    "authors": [
        {
            "name": "Jaanus Vapper",
            "email": "hullkuri@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hulkur\\HasManyKeyBy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hulkur\\HasManyKeyBy\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hulkur\\HasManyKeyBy\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel"
    },
    "config": {
        "sort-packages": true
    }
}
